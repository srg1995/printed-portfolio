---
import Section from "../Section.astro";
import { skills } from "@cv";
---

<Section title="Habilidades">
  <ul class="inline-flex flex-wrap gap-2 mb-4">
    {
      skills.map(({ name }) => {
        return (
          <li class="rounded-md bg-gray-200 text-black font-medium py-1 px-2">
            <p>{name}</p>
          </li>
        );
      })
    }
  </ul>
</Section>
